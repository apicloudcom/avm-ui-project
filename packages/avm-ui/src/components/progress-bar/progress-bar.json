    {
      "name": "trackColor",
      "type": "string",
      "tip": "线条的颜色",
      "default": "#1677ff"
    },
    {
      "name": "fillColor",
      "type": "string",
      "tip": "填充的颜色",
      "default": "#E5E5E5"
    }
  ]
}