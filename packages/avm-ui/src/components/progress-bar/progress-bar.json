{
  "component": "ProgressBar",
  "desc": "progress-bar 组件定义一个进度条",
  "type": 2,
  "props": [
    {
      "name": "percent",
      "type": "number",
      "tip": "百分比",
      "default": 30
    },
    {
      "name": "trackWidth",
      "type": "number",
      "tip": "线条宽度",
      "default": 10
    },
    {
      "name": "borderRadius",
      "type": "number",
      "tip": "圆角大小",
      "default": 3
