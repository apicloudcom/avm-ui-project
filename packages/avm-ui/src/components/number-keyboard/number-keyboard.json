{
    "component": "NumberKeyboard",
    "desc": "数字键盘",
    "type": 3,
    "event": [
        "handleInput",
        "handleDelete",
        "handleClose",
        "handleConfirm",
        "afterClose",
        "clearAll"
    ],
    "props": [
        {
            "name": "type",
            "tip": "键盘类型（default:默认、ID:身份证号、custom: 自定义）",
            "type": "string",
            "enum": ["default", "ID", "custom"],
            "default": "default"
        },
        {
            "name": "visible",
            "tip": "是否展示",
            "type": "boolean",
            "default": false
        },
        {
            "name": "randomOrder",
            "tip": "是否乱序键盘",
            "type": "boolean",
            "default": false
        },
        {
            "name": "showCloseButton",
            "tip": "自定义样式状态是否显示收起键盘按钮",
            "type": "boolean",
            "default": true
        },
        {
            "name": "closeOnConfirm",
            "tip": "是否在点击确定按钮时自动关闭",
            "type": "boolean",
            "default": true
        },
        {
            "name": "isShowClearBtn",
            "tip": "是否显示清空按钮",
            "type": "boolean",
            "default": true
        }
    ]
}