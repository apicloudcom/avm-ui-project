{
    "component": "TabBarItem",
    "desc": "标签栏-子标签",
    "parent": "tab-bar",
    "type": 2,
    "event": [
        "onChange"
    ],
    "props": [
        {
            "name": "key",
            "tip": "对应activeKey",
            "type": "string",
            "default": "1"
        },
        {
            "name": "title",
            "tip": "标题",
            "type": "string",
            "default": "选项"
        },
        {
            "name": "icon",
            "tip": "图标",
            "type": "string"
        },
        {
            "name": "badgeContent",
            "tip": "徽标内容，同 [Badge](./badge) 的 `content` 属性，不传则不显示徽标",
            "type": "string"
        },
        {
            "name": "isDot",
            "tip": "徽标是否显示小红点，如果设置为true,则显示小红点，并且badgeContent则不生效",
            "type": "boolean"
        }
    ]
}