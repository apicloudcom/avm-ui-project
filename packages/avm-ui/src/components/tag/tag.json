{
  "component": "Tag",
  "desc": "标签",
  "type": 2,
  "event": [
    "handleClick"
  ],
  "props": [
    {
      "name": "children",
      "tip": "标签文本",
      "type": "string",
      "default": "标签",
      "generateData": true
    },
    {
      "name": "color",
      "tip": "标签色",
      "enum": [
        "default",
        "primary",
        "success",
        "warning",
        "danger",
        "string"
      ],
      "type": "string",
      "default": "primary",
      "generateData": true
    },
    {
      "name": "fill",
      "tip": "填充模式",
      "type": "string",
      "enum": [
        "solid",
        "outline"
