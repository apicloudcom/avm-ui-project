      "type": "boolean",
      "default": false
    },
    {
      "name": "textColor",
      "tip": "文字颜色",
      "type": "string",
      "default": "#666"
    },
    {
      "name": "bgColor",
      "tip": "背景颜色",
      "type": "string"
    },
    {
      "name": "borderColor",
      "tip": "边框颜色",
      "type": "string"
    },
    {
      "name": "borderRadius",
      "tip": "round=false时的圆角大小",
      "type": "string",
      "default": "2px"
