{
    "component": "CheckList",
    "desc": "可勾选列表",
    "type": 2,
    "event": [
        "handleChange"
    ],
    "props": [
        {
            "name": "defaultValue",
            "tip": "默认选中项",
            "type": "array",
            "default": []
        },
        {
            "name": "multiple",
            "tip": "是否允许多选",
            "type": "boolean",
            "default": false
        },
        {
            "name": "readOnly",
            "tip": "是否只读",
            "type": "boolean",
            "default": false
        },
        {
            "name": "disabled",
            "tip": "是否禁用",
            "type": "boolean",
            "default": false
        },
        {
            "name": "list",
            "tip": "选项列表, 格式为[{value: 选项值, label: 选项text值, readonly: 是否只读, disabled: 是否禁用}]",
            "type": "array",
            "default": [{
                "value": "one",
                "label": "选项1",
                "readonly": false,
                "disabled": false
            },{
                "value": "two",
                "label": "选项2",
                "readonly": false,
                "disabled": false
            },{
                "value": "three",
                "label": "选项3",
                "readonly": false,
                "disabled": false
            }]
        },
        {
            "name": "activeIcon",
            "tip": "选中图标",
            "type": "string"
        }
    ]
}